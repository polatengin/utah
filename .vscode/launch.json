{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "cwd": "${workspaceFolder}/src/vscode-extension",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/src/vscode-extension",
      ],
      "outFiles": [
        "${workspaceFolder}/src/vscode-extension/dist/**/*.js"
      ],
      "preLaunchTask": "${defaultBuildTask}"
    }
  ]
}
