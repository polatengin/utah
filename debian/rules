#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_clean:
	@make clean

override_dh_auto_build:
	@make build

override_dh_auto_test:
	@echo "Tests skipped"

override_dh_dwz:
	@echo "DWZ compression skipped"

override_dh_shlibdeps:
	@dh_shlibdeps --dpkg-shlibdeps-params=--ignore-missing-info --dpkg-shlibdeps-params=--warnings=0 -Xlibdl.so.2 -Xlibpthread.so.0
