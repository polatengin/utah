// Test function call compilation
function demonstrateStringFunctions(): void {
  console.log("Demonstrating string functions");
}

function greet(name: string): void {
  console.log(`Hello, ${name}!`);
}

let userName: string = "Alice";

// Function calls
demonstrateStringFunctions();
greet("World");      // String literal - should be greet "World"
greet(userName);     // Variable reference - should be greet "$userName"
