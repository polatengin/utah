let text: string = "Hello World";
let length: number = string.length(text);
let upper: string = string.toUpperCase(text);
let lower: string = string.toLowerCase(text);
console.log("Text: " + text);
console.log("Length: " + length);
console.log("Upper: " + upper);
console.log("Lower: " + lower);

let logPath: string = "/var/log/nginx.log";
console.log(logPath);

if (string.startsWith(logPath, "/var")) {
  console.log("Log is in var directory");
}

let filename: string = "test.txt";
if (string.endsWith(filename, ".txt")) {
  console.log("Text file detected");
}
